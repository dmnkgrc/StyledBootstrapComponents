{
  "name": "styled-react-bootstrap",
  "version": "0.0.1",
  "description": "Bootstrap components built with React and Styled Components",
  "main": "",
  "scripts": {
    "start": "parcel src/index.js",
    "build": "parcel build src/index.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "flow": "flow check",
    "flow:watch": "flow-watch",
    "format": "eslint ./**/*.js --fix",
    "lint": "eslint src",
    "prettier": "prettier */**/*.js --write"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "repository": "git+https://github.com/dmnkgrc/StyledBootstrapComponents.git",
  "keywords": [
    "react",
    "styled-components",
    "bootstrap",
    "css-in-js",
    "css"
  ],
  "author": "Dominik Garc√≠a",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmnkgrc/StyledBootstrapComponents/issues"
  },
  "homepage": "https://github.com/dmnkgrc/StyledBootstrapComponents#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@emotion/memoize": "^0.7.1",
    "@emotion/sheet": "^0.9.2",
    "@emotion/stylis": "^0.8.3",
    "@emotion/utils": "^0.11.1",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "chai": "^4.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.2",
    "flow-bin": "^0.89.0",
    "jest": "^23.6.0",
    "jest-image-snapshot": "^2.7.0",
    "jest-styled-components": "^6.3.1",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-test-renderer": "^16.7.0",
    "styled-components": "^4.1.3",
    "styled-props": "^1.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow check && lint"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "./src/setupTests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "babel-core": "^7.0.0-bridge.0"
  }
}
